    "children": [
      "product-highlight-wrapper#shelf__free-shipping-wrapper"
    ],
    "props": {
      "blockClass": "custom_highlights",
      "filter": {
        "type": "show",
        "highlightNames": [
          "Env√≠o Gratis"
        ]
      }
    }
  },
